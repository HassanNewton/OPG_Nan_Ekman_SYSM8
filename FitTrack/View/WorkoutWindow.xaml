<Window x:Class="FitTrack.View.WorkoutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitTrack.View"
        mc:Ignorable="d"
        Title="WorkoutWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="USER" Width="150" Height="40" Background="Crimson" FontFamily="Arial" Command="{Binding OpenUserDetailsCommand}" Opacity="0.8"/>
            <Button Content="ADD WORKOUT" Width="150" Height="40" Background="Crimson" FontFamily="Arial" Command="{Binding AddWorkOutCommand}" Opacity="0.8"/>
            <Button Content="DETAILS" Width="150" Height="40" Background="Crimson" FontFamily="Arial" Command="{Binding OpenWorkoutDetailsWindowCommand}" Opacity="0.8"/>
        </StackPanel>
    </Grid>
</Window>
